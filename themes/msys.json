{
	"plugins": [ "git", "ssh", "z", "aliases" ],
	"prompt": [
		[ "darkgreen",   "", "$user@$hostname " ],
		[ "darkyellow",  "", "$path " ],
		[ "",            "", "$git_lbracket$git_branch" ],
		[ "red",         "", "$git_dirty" ],
		[ "",            "", "$git_rbracket "],
		[ "",            "", "`nPS>" ]
	],
	"git": {
		"prompt_dirty": "*",
		"prompt_lbracket": "[",
		"prompt_rbracket": "]"
	}
}