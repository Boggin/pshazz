{
	"plugins": [ "git", "hg", "ssh", "z", "aliases" ],
	"prompt": [
		[ "darkcyan",  "", "$dir" ],
		[ "darkred",   "", " $git_branch$git_dirty" ],
		[ "darkred",   "", " $hg_branch" ],
		[ "",          "", "$hg_bookmark" ],
		[ "darkred",   "", "$hg_dirty" ],
		[ "darkgreen", "", " `$" ]
	],
	"git": {
		"prompt_dirty": "*"
	},
	"hg": {
		"prompt_dirty": "*"
	}
}