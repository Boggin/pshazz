{
	"plugins": [ "git", "ssh", "z" ],
	"prompt": [
		[ "darkgreen", "", "$user@$hostname " ],
		[ "darkyellow",  "", "$dir " ],
		[ "red",   "", "$git_lbracket$git_branch$git_dirty$git_rbracket " ],
		[ "", "", "`nPS>" ]
	],
	"git": {
		"prompt_dirty": "*",
		"prompt_lbracket": "[",
		"prompt_rbracket": "]"
	}
}